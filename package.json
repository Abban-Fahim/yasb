{
  "name": "yasb",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "pretauri": "npm run cargo:test && npm run lint",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --emptyOutDir",
    "tauri": "cross-env TAURI_DEV_WATCHER_IGNORE_FILE=.tauriignore tauri",
    "lint": "npm run eslint && npm run rustfmt",
    "eslint": "eslint src --ext .ts,.vue --max-warnings=0 --fix",
    "rustfmt": "rustfmt ./src-tauri/src/main.rs",
    "cargo:test": "cargo test --manifest-path ./src-tauri/Cargo.toml"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.1.0",
    "humanize-duration": "^3.27.2",
    "moment": "^2.29.4",
    "pretty-bytes": "^6.0.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.1.1",
    "@types/humanize-duration": "^3.27.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "sass": "^1.54.4",
    "typescript": "^4.6.4",
    "url": "^0.11.0",
    "vite": "^3.0.0",
    "vite-plugin-vue-type-imports": "^0.2.0",
    "vue-eslint-parser": "^9.0.3",
    "vue-tsc": "^0.38.4"
  }
}
